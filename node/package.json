{
  "name": "group-watch-bot",
  "version": "0.0.1",
  "description": "Discord bot to manage group watching of tv shows",
  "main": "./dist/index.js",
  "scripts": {
    "exec:run": "node . run",
    "exec:updateguildcommands": "node . updateguildcommands",
    "exec:clearguildcommands": "node . clearguildcommands",
    "exec:updateglobalcommands": "node . updateglobalcommands",
    "exec:clearglobalcommands": "node . clearglobalcommands",
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "npx tsc",
    "build:watch": "npx tsc --watch",
    "build:clean": "npx tsc --build --clean"
  },
  "keywords": [
    "discord",
    "discord.js",
    "group-watching",
    "tv-shows"
  ],
  "author": "lpchaim@gmail.com",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@discordjs/builders": "^0.5.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@discordjs/voice": "^0.6.0",
    "bufferutil": "^4.0.3",
    "discord-api-types": "^0.22.0",
    "discord.js": "^13.1.0",
    "erlpack": "github:discord/erlpack",
    "utf-8-validate": "^5.0.5",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.0",
    "eslint": "^7.32.0",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5"
  }
}
